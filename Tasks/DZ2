package ru.itpark;

public class Main {

    public static void main(String[] args) {
        int array[] = new int[5];
	    array[0] = 7;
	    array[1] = 2;
	    array[2] = -3;
	    array[3] = 1;
	    array[4] = 5;
        int arrayInPlaceInitialized[] =
                {7, 3, -3, 1, 5};

        int i = 0;
        // пока i меньше пяти
        while (i < 5) {
            // печатаем a[i]
            System.out.print(array[i] + " ");
          }
    }
}